{{!< default}}

{{! The comment above "< default" means - insert everything in this file into
    the {body} of the default.hbs template, which contains our header/footer. }}

<main id="home" class="main" role="main">
	
	<!-- start: .center -->
	<div class="center content grid-container">
	
		{{> carousel}}

		{{> breaking-news }}
		
		<!-- start: .ads -->
			<script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- Top Banner -->
<ins class="adsbygoogle"
     style="display:inline-block;width:970px;height:90px"
     data-ad-client="ca-pub-8236710384567365"
     data-ad-slot="4646156948"></ins>
<script>
     (adsbygoogle = window.adsbygoogle || []).push({});
</script>
<amp-ad
     layout="fixed"
     width="970"
     height="90"
     type="adsense"
     data-ad-client="ca-pub-8236710384567365"
     data-ad-slot="4646156948">
</amp-ad>
		<!-- end: .ads -->
		
		
		<!-- start: .left-content -->
		<div class="left-content section articles grid-parent grid-50">
			<h3 class="title bordered">RECENT NEWS</h3>
			
			{{#foreach posts}}
				{{#if @first}}
					<article class="first {{post_class}}">
						<header>
							{{#if feature_image}}
								<a href="{{url}}" class="thumb hover-effect">
									<img src="{{feature_image}}">
								</a>
							{{/if}}
							<div class="clear"></div>
							
							<div class="meta">
								<div class="alignleft">
									{{#if tags}}
										<div class="tags">
											{{tags separator=""}}
										</div>
									{{/if}}
									<time datetime="{{date format="YYYY-MM-DD"}}"><i class="fa fa-clock-o"></i>{{date}}</time>
								</div>
							<h4 class="title"><a href="{{url}}">{{{title}}}</a></h4>
						</header>
						<div class="post-excerpt">
							{{excerpt}}
							<div class="clear"></div>
						</div> 
						<a href="{{url}}" class="button dark">Continue Reading</a>
						<a href="javascript:void(0)" class="button dark share-button"><i class="fa fa-share-alt"></i> SHARE</a>
						<div class="tip-content">
							<div class="share">
								<a class="facebook" href="https://www.facebook.com/sharer/sharer.php?u={{url absolute="true"}}"
							onclick="window.open(this.href, 'facebook-share','width=580,height=296');return false;"><i class="fa fa-facebook"></i></a>
								<a class="twitter" href="http://twitter.com/share?text={{encode title}}&url={{url absolute="true"}}"
							onclick="window.open(this.href, 'twitter-share', 'width=550,height=235');return false;"><i class="fa fa-twitter"></i></a>
								<a class="googleplus" href="https://plus.google.com/share?url={{url absolute="true"}}"
						   onclick="window.open(this.href, 'google-plus-share', 'width=490,height=530');return false;"><i class="fa fa-google-plus"></i></a>
							</div>
						</div>
					</article>
				{{else}}
				
					{{> default-article}}
					
				{{/if}}
			{{/foreach}}
			
			{{pagination}}
			
		</div>
		<!-- end: .left-content -->
		
		<div class="section">
			
			{{> sidebar-featured}}
			
			{{> sidebar-default}}
		
		</div>
	
	</div>
	<!-- end: .center -->
	

		<script type="text/javascript" src="//downloads.mailchimp.com/js/signup-forms/popup/unique-methods/embed.js" data-dojo-config="usePlainJson: true, isDebug: false"></script><script type="text/javascript">window.dojoRequire(["mojo/signup-forms/Loader"], function(L) { L.start({"baseUrl":"mc.us5.list-manage.com","uuid":"d26225c0c7c87bc20295436eb","lid":"9c4c243686","uniqueMethods":true}) })</script>

</main>
